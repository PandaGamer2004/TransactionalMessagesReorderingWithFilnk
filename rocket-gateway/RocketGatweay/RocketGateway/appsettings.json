{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "KafkaConfiguration": {
    "Shared": {
      "BootstrapServer": "localhost:9092"
    },
    "Producer": {
      "Acks": "All",
      "Topic": "rocket-updates"
    },
    "Consumer": {
      "Topic": "rocket-reordered-updates",
      "Workers": 5,
      "BufferSize": 100,
      "GroupId": "rocket-updates",
      "AutoOffsetReset": "Latest"
    }
  },
  "AllowedHosts": "*"
}
